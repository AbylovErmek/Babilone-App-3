<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.profile.ProfileFragment">

    <LinearLayout
        android:id="@+id/linearColumn"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rectangle_bg_white_a700_01"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            app:contentInsetStart="0dp">

            <LinearLayout
                android:id="@+id/linearRow"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="122dp"
                android:layout_marginTop="11dp"
                android:layout_marginEnd="24dp"
                android:layout_marginBottom="11dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt"
                    style="@style/txtIntersemibold16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top"
                    android:layout_marginBottom="2dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="15sp"
                    tools:text="Личный кабинет" />

                <ImageView
                    android:id="@+id/imageMaterialsymbol"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="115dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_material_symbol"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_material_symbol" />
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linearColumnview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingStart="24dp"
            android:paddingTop="7dp"
            android:paddingEnd="24dp"
            android:paddingBottom="7dp">

            <View
                android:id="@+id/viewView"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/rectangle_bg_blue_gray_100_01_radius_50" />

            <TextView
                android:id="@+id/txt1"
                style="@style/txtIntersemibold18_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="12dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:textColor="@color/black"
                tools:text="Виктория" />

            <TextView
                android:id="@+id/txtEmail"
                style="@style/txtIntermedium16_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="3dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:textColor="@color/black"
                tools:text="@string/msg_victoria_gmai_com" />

            <ImageView
                android:id="@+id/iv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="TODO" />

            <EditText
                android:id="@+id/etFirsbell"
                style="@style/etSolidRounded_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="@drawable/rectangle_bg_gray"
                android:drawableStart="@drawable/img_firsbell"
                android:drawableEnd="@drawable/img_firsangleright_black_900"
                android:drawablePadding="30dp"
                android:ellipsize="end"
                android:inputType="text"
                android:paddingStart="15dp"
                android:paddingTop="13dp"
                android:paddingEnd="15dp"
                android:paddingBottom="13dp"
                android:singleLine="true"
                android:text="@string/EditText"
                android:textColorHint="@color/gray"
                tools:ignore="Autofill" />

            <LinearLayout
                android:id="@+id/linearRowfirslockOne"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="@drawable/rectangle_bg_gray_200"
                android:drawableEnd="@drawable/img_firsangleright_black_900"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingStart="15dp"
                android:paddingTop="11dp"
                android:paddingEnd="15dp"
                android:paddingBottom="11dp">

                <ImageView
                    android:id="@+id/imageFirslockOne"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_fi_rs_lock"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_fi_rs_lock" />

                <TextView
                    android:id="@+id/txt2"
                    style="@style/txtIntermedium15_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="2dp"
                    android:ellipsize="end"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    tools:text="@string/msg40" />

                <ImageView
                    android:id="@+id/imageFirsangleOne"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_firsangleright_black_900"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_firsangleright_black_900" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowfirsmoonOne"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="@drawable/rectangle_bg_gray_200"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingStart="15dp"
                android:paddingTop="7dp"
                android:paddingEnd="15dp"
                android:paddingBottom="7dp">

                <ImageView
                    android:id="@+id/imageFirsmoonOne"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="7dp"
                    android:layout_marginBottom="7dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_fi_rs_moon"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_fi_rs_moon" />

                <TextView
                    android:id="@+id/txt3"
                    style="@style/txtIntermedium15_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="6dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    tools:text="@string/lbl68" />

                <com.google.android.material.switchmaterial.SwitchMaterial
                    android:id="@+id/switchMaterialSwitchOne"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="180dp" />
            </LinearLayout>

            <EditText
                android:id="@+id/etLogoutOne"
                style="@style/etSolidRounded_4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/rectangle_bg_gray_200"
                android:drawableStart="@drawable/img_firssignoutalt"
                android:drawablePadding="10dp"
                android:ellipsize="end"
                android:hint="@string/Goout"
                android:inputType="text"
                android:paddingStart="15dp"
                android:paddingTop="13dp"
                android:paddingEnd="30dp"
                android:paddingBottom="13dp"
                android:singleLine="true"
                android:textColorHint="@color/red"
                tools:ignore="Autofill" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>